{
  "name": "YourTube",
  "version": "1.0",
  "description": "HackGT Project",
  "permissions": ["declarativeContent", "storage", "activeTab", "tabs", "<all_urls>", "alarms"],
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "YourTube.png",
      "32": "YourTube.png",
      "48": "YourTube.png",
      "128": "YourTube.png"
    }
  },
  "icons": {
    "16": "YourTube.png",
    "32": "YourTube.png",
    "48": "YourTube.png",
    "128": "YourTube.png"
  },
  "commands": {
    "toggle-speedUp": {
      "suggested_key": {
        "default": "Ctrl+Right",
        "mac": "Command+Right"
      },
      "description": "Toggle speedUp"
    },
    "toggle-speedDown": {
      "suggested_key": {
        "default": "Ctrl+Left",
        "mac": "Command+Left"
      },
      "description": "Toggle speedDown"
    },
    "toggle-mirror": {
      "suggested_key": {
        "default": "Ctrl+Down",
        "mac": "Command+Down"
      },
      "description": "Toggle mirror"
    },
    "toggle-rotate90": {
      "suggested_key": {
        "default": "Ctrl+Up",
        "mac": "Command+Up"
      },
      "description": "Toggle quick rotate90"
    }
  },
  "manifest_version": 2
}
